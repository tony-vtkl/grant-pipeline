# Grant Ingestion Pipeline Configuration
# Copy this file to .env and fill in your values

# Required: Supabase project URL
SUPABASE_URL=https://your-project.supabase.co

# Required: Supabase API key (anon or service_role)
SUPABASE_KEY=your-supabase-key

# Required: SAM.gov API key
SAM_API_KEY=your-sam-api-key

# Required: Grants.gov attribution header (per ToS)
GRANTS_GOV_ATTRIBUTION=VTKL Grant Pipeline

# Optional: Polling interval in minutes (default: 60)
POLLING_INTERVAL_MINUTES=60

# Optional: Log level (default: INFO)
LOG_LEVEL=INFO
